                app:layout_constraintTop_toTopOf="@+id/tv_pickup_extra_stops_count4"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/tv_pickup_extra_stops_count6"
                style="@style/HomeTextLightGrey"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/storage"
                app:layout_constraintBottom_toBottomOf="@+id/tv_pickup_extra_stops_count5"
                app:layout_constraintEnd_toEndOf="@+id/txt_estimate_hours"
