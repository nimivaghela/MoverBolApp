            <TextView
                android:id="@+id/txt_estimate_hours"
                style="@style/HomeTextBlack"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@{obj.estimatedHours}"
                app:layout_constraintBottom_toBottomOf="@+id/txt_hour_rate"
                app:layout_constraintEnd_toEndOf="@+id/tv_pickup_extra_stops_count0"
                app:layout_constraintStart_toStartOf="@+id/tv_pickup_extra_stops_count0"
                app:layout_constraintTop_toTopOf="@+id/txt_hour_rate"
                app:layout_constraintVertical_bias="0.0"
