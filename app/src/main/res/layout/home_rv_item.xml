                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text='@{obj.unpacking.equals("1") ? "Yes" : "No"}'
                app:layout_constraintBottom_toBottomOf="@+id/txt_packing"
                app:layout_constraintEnd_toEndOf="@+id/tv_pickup_extra_stops_count5"
                app:layout_constraintStart_toStartOf="@+id/tv_pickup_extra_stops_count5"
                app:layout_constraintTop_toTopOf="@+id/txt_packing"
                app:layout_constraintVertical_bias="0.0"
                tools:text="Yes/No" />

            <TextView
                android:id="@+id/tv_pickup_extra_stops_count9"
                style="@style/HomeTextBlack"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text='@{obj.storage.equals("1") ? "Yes" : "No"}'
                app:layout_constraintBottom_toBottomOf="@+id/tv_pickup_extra_stops_count8"
                app:layout_constraintEnd_toEndOf="@+id/tv_pickup_extra_stops_count6"
                app:layout_constraintStart_toStartOf="@+id/tv_pickup_extra_stops_count6"
                app:layout_constraintTop_toTopOf="@+id/tv_pickup_extra_stops_count8"
                app:layout_constraintVertical_bias="1.0"
                tools:text="Yes/No" />

            <TextView
                android:id="@+id/txt_head_min_hours"
                style="@style/HomeTextLightGrey"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/home_est_margin_top"
                android:text="@string/min_hours"
                app:layout_constraintEnd_toEndOf="@+id/txt_packing"
                app:layout_constraintStart_toStartOf="@+id/txt_packing"
                app:layout_constraintTop_toBottomOf="@id/txt_packing" />

            <TextView
                android:id="@+id/txt_head_min_charges"
                style="@style/HomeTextLightGrey"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/min_charges"
                app:layout_constraintBottom_toBottomOf="@+id/txt_head_min_hours"
                app:layout_constraintEnd_toEndOf="@+id/tv_pickup_extra_stops_count5"
                app:layout_constraintStart_toStartOf="@+id/tv_pickup_extra_stops_count5"
                app:layout_constraintTop_toTopOf="@+id/txt_head_min_hours" />

            <TextView
                android:id="@+id/txt_head_total_charges"
                style="@style/HomeTextLightGrey"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                visibleInvisible="@{obj.estimateShowFlag == 1}"
                android:text="@string/total_est_charges"
                app:layout_constraintEnd_toEndOf="@+id/tv_pickup_extra_stops_count6"
                app:layout_constraintStart_toStartOf="@+id/tv_pickup_extra_stops_count6"
                app:layout_constraintTop_toTopOf="@+id/txt_head_min_charges" />

            <TextView
                android:id="@+id/txt_min_hours"
                style="@style/HomeTextBlack"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8sdp"
                android:text="@{obj.minHours}"
                app:layout_constraintEnd_toEndOf="@+id/txt_head_min_hours"
                app:layout_constraintStart_toStartOf="@id/txt_head_min_hours"
                app:layout_constraintTop_toBottomOf="@id/txt_head_min_hours"
                tools:text="1.5" />

            <TextView
                android:id="@+id/txt_min_charges"
                style="@style/HomeTextBlack"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8sdp"
                android:text="@{currencySymbol+obj.minCharges}"
                app:layout_constraintEnd_toEndOf="@+id/txt_head_min_charges"
                app:layout_constraintStart_toStartOf="@id/txt_head_min_charges"
                app:layout_constraintTop_toBottomOf="@id/txt_head_min_charges"
                tools:text="$200.0" />

            <TextView
                android:id="@+id/txt_total_charges"
                style="@style/HomeTextBlack"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8sdp"
                visibleInvisible="@{obj.estimateShowFlag == 1}"
                android:text="@{currencySymbol+obj.totalCharges}"
                app:layout_constraintEnd_toEndOf="@+id/txt_head_total_charges"
                app:layout_constraintStart_toStartOf="@id/txt_head_total_charges"
                app:layout_constraintTop_toBottomOf="@id/txt_head_total_charges"
                tools:text="$200.0" />

            <View
                android:id="@+id/view2"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="@dimen/_32sdp"
                android:background="@color/divider_grey"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txt_min_charges" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.5" />


            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:visibility="@{obj.jobStatus.equals(Constants.JOB_STATUS_ACCEPTED) || obj.jobStatus.equals(Constants.JOB_STATUS_INPROGRESS)? View.VISIBLE : View.GONE}"
                android:weightSum="2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/view2">


                <TextView
                    android:id="@+id/tv_summary_big"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_70sdp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:onClick="@{onClick}"
                    android:tag="@{obj.jobId}"
                    android:text="@string/summary"
                    android:textAllCaps="true"
                    android:textColor="@color/home_dark_grey"
                    android:textSize="@dimen/textsize_22sp" />
                <!--&lt;!&ndash;&lt;!&ndash;&lt;!&ndash;android:visibility="@{obj.jobStatus.equals(Constants.JOB_STATUS_ACCEPTED) ? View.VISIBLE : View.GONE}" />&ndash;&gt;&ndash;&gt;&ndash;&gt;-->

                <TextView
                    android:id="@+id/tv_start_move"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_70sdp"
                    android:layout_weight="1"
                    android:background="@color/colorPrimary"
                    android:gravity="center"
                    android:onClick="@{onClick}"
                    android:tag="@{position}"
                    android:text="@string/start_move"
                    android:textAllCaps="true"
                    android:textColor="@color/colorAccent"
                    android:textSize="@dimen/textsize_22sp" />

                <!--  &lt;!&ndash;&lt;!&ndash; &lt;!&ndash;android:visibility="@{obj.jobStatus.equals(Constants.JOB_STATUS_ACCEPTED) ? View.VISIBLE : View.GONE}" />&ndash;&gt;
               &lt;!&ndash;app:layout_constraintEnd_toEndOf="parent"
               app:layout_constraintStart_toStartOf="parent"
               app:layout_constraintTop_toBottomOf="@+id/view2" />&ndash;&gt;&ndash;&gt;&ndash;&gt;-->


            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:visibility="@{obj.jobStatus.equals(Constants.JOB_STATUS_NEW) ? View.VISIBLE : View.GONE}"
                android:weightSum="3"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/view2">

                <TextView
                    android:id="@+id/tv_summary"
                    android:layout_width="0dp"
                    android:layout_height="72dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:onClick="@{onClick}"
                    android:text="@string/summary"
                    android:textAllCaps="true"
                    android:textColor="@color/home_dark_grey"
                    android:textSize="@dimen/textsize_20sp"
                    android:visibility="@{obj.jobStatus.equals(Constants.JOB_STATUS_NEW) ? View.VISIBLE : View.GONE}" />
                <!--&lt;!&ndash;&lt;!&ndash; &lt;!&ndash;app:layout_constraintEnd_toEndOf="parent"
                 app:layout_constraintRight_toRightOf="parent"
                 app:layout_constraintStart_toStartOf="@+id/guideline"
                 app:layout_constraintTop_toBottomOf="@+id/view2" />&ndash;&gt;&ndash;&gt;&ndash;&gt;-->


                <TextView
                    android:id="@+id/tv_accept"
                    android:layout_width="0dp"
                    android:layout_height="72dp"
                    android:layout_weight="1"
                    android:background="@color/colorPrimary"
                    android:gravity="center"
                    android:onClick="@{onClick}"
                    android:text="@string/accept"
                    android:textAllCaps="true"
                    android:textColor="@color/colorAccent"
                    android:textSize="@dimen/textsize_20sp" />
                <!--     &lt;!&ndash;&lt;!&ndash; &lt;!&ndash;android:visibility="@{obj.jobStatus.equals(Constants.JOB_STATUS_NEW) ? View.VISIBLE : View.GONE}" />&ndash;&gt;
                  &lt;!&ndash;app:layout_constraintBottom_toBottomOf="@+id/tv_reject"
                  app:layout_constraintEnd_toStartOf="@+id/guideline"
                  app:layout_constraintLeft_toLeftOf="parent"
                  app:layout_constraintStart_toStartOf="parent"
                  app:layout_constraintTop_toTopOf="@+id/tv_reject"
                  app:layout_constraintVertical_bias="0.0" />&ndash;&gt;&ndash;&gt;&ndash;&gt;-->

                <TextView
                    android:id="@+id/tv_reject"
                    android:layout_width="0dp"
                    android:layout_height="72dp"
                    android:layout_weight="1"
                    android:background="@color/light_green"
                    android:gravity="center"
                    android:onClick="@{onClick}"
                    android:text="@string/reject"
                    android:textAllCaps="true"
                    android:textColor="@color/colorAccent"
                    android:textSize="@dimen/textsize_20sp" />
                <!--   &lt;!&ndash;   &lt;!&ndash; &lt;!&ndash;android:visibility="@{obj.jobStatus.equals(Constants.JOB_STATUS_NEW) ? View.VISIBLE : View.GONE}" />&ndash;&gt;
                   &lt;!&ndash;app:layout_constraintEnd_toEndOf="parent"
                   app:layout_constraintRight_toRightOf="parent"
                   app:layout_constraintStart_toStartOf="@+id/guideline"
                   app:layout_constraintTop_toBottomOf="@+id/view2" />&ndash;&gt;&ndash;&gt;&ndash;&gt;-->


            </LinearLayout>


            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.5" />

            <TextView
                android:id="@+id/textView8"
                style="@style/HomeTextLightGrey"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="@dimen/home_activity"
                android:gravity="center_vertical"
                android:text=""
                android:visibility="invisible"
                app:layout_constraintBottom_toBottomOf="@+id/tv_id"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/tv_id"
                app:layout_constraintVertical_bias="1.0"
                tools:text="@string/activity" />

            <TextView
                android:id="@+id/textView30"
                style="@style/HomeTextLightGrey"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_5sdp"
                android:gravity="center_vertical"
                android:text="@string/service_type"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="@+id/textView8"
                app:layout_constraintRight_toLeftOf="@+id/tv_local"
                app:layout_constraintTop_toTopOf="@+id/textView8"
                app:layout_constraintVertical_bias="0.0" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>
</layout>